pipeline {
agent any

stages {
 

         stage('Cloning from GitHub') {
                steps {
                    git branch: 'rania_categorie', url: 'https://github.com/semah1213/achat'
                }  
            }
           
               stage('maven clean') {
                        steps {
                           sh 'mvn clean '
                        }
                    }
          stage('maven compile') {
            steps {
               sh 'mvn compile'
           }
        }

          stage('maven test / MOCKITO') {
            steps {
               sh 'mvn test'
            }
        }

}
   

}